<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="19" not-null="true">
		</sql-type>
		<sql-type jdbc-type="BIT" hibernate-type="boolean"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="REAL" hibernate-type="float" precision="12"
			not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-name="address" match-catalog="ekart"></table-filter>
	<table-filter match-name="category" match-catalog="ekart"></table-filter>
	<table-filter match-name="country" match-catalog="ekart"></table-filter>
	<table-filter match-name="lookup_type" match-catalog="ekart"></table-filter>
	<table-filter match-name="measuring_unit" match-catalog="ekart"></table-filter>
	<table-filter match-name="order_lines" match-catalog="ekart"></table-filter>
	<table-filter match-name="orders" match-catalog="ekart"></table-filter>
	<table-filter match-name="product" match-catalog="ekart"></table-filter>
	<table-filter match-name="retailer_snapshot"
		match-catalog="ekart">
	</table-filter>
	<table-filter match-name="roles" match-catalog="ekart"></table-filter>
	<table-filter match-name="user" match-catalog="ekart"></table-filter>
	<table-filter match-name="user_role" match-catalog="ekart"></table-filter>
	<table-filter match-name="order_history" match-catalog="ekart"></table-filter>
	<table-filter match-name="organization" match-catalog="ekart"></table-filter>
	<table-filter match-name="user_address" match-catalog="ekart"></table-filter>
</hibernate-reverse-engineering>